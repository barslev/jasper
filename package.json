{
  "name": "@kuremichi/jasper",
  "version": "0.1.6",
  "description": "Jasper Rule Engine",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "test": "jest --config jest.config.json --testPathPattern \"(/__tests__/.*\\.(test|spec))\\.(jsx?|tsx?)$\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint .",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuremichi/jasper.git"
  },
  "keywords": [
    "Jasper",
    "Engine"
  ],
  "author": "Evan Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuremichi/jasper/issues"
  },
  "homepage": "https://github.com/kuremichi/jasper#readme",
  "dependencies": {
    "jsonata": "^1.8.4",
    "lodash": "^4.17.21",
    "object-hash": "^2.1.1",
    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.7",
    "@types/jest": "^26.0.20",
    "@types/jsonata": "^1.5.1",
    "@types/lodash": "^4.14.168",
    "@types/moment": "^2.13.0",
    "@types/object-hash": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typedoc": "^0.20.33",
    "typescript": "^4.2.3"
  },
  "files": [
    "lib/**/*"
  ]
